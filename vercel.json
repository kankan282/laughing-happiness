{
  "version": 2,
  "builds": [
    {
      "src": "api/predict.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/predict",
      "dest": "/api/predict.py"
    },
    {
      "src": "/",
      "dest": "/api/predict.py"
    }
  ],
  "functions": {
    "api/predict.py": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
